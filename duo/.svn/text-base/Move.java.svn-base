package duo;

/** Class that holds a single move and it's value.
 * @author Ken Szubzda (cs61b-ek)
 */
class Move {

    /** Constructor that takes a string MOVE and VALUE heuristic. */
    Move(String move, int value) {
        _move = move;
        _value = value;
    }

    /** Return the value of move. */
    String getMove() {
        return _move;
    }
    /** Return the value of the heuristic. */
    int getValue() {
        return _value;
    }
    /** Sets the VAL value of the heuristic. */
    void setValue(int val) {
        _value = val;
    }

    /** String instance variable that stores the move. */
    private String _move;
    /** Move instance variable that stores the heuristic value. */
    private int _value;
}
